container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py migrate && python3 manage.py migrate_categories_data"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: spritacular.wsgi:application
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: staticfiles/